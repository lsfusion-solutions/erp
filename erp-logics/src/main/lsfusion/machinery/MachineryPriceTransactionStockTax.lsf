MODULE MachineryPriceTransactionStockTax;

REQUIRE MachineryPriceTransaction, StockTax;

NAMESPACE Machinery;

skipVATMachinery = ABSTRACT BOOLEAN (Barcode);
VAT(Barcode barcode) = valueVAT(sku(barcode), defaultCountry(), currentDate()) IF NOT skipVATMachinery(barcode);
@defineMachineryPriceTransactionAttribute(VAT, 'НДС, %', NUMERIC[10,5]);