MODULE CashBookPrint;

REQUIRE CashBookIncome, CashBookOutcome;

NAMESPACE CashOperation;

FORM printCashBookList
    OBJECTS l = CashBookList PANEL
    PROPERTIES(l) date, time, nameDepartmentStore, 
                  startBalance, endBalance,
                  sumIncome, sumOutcome,
                  countIncome, countOutcome,
                  nameEmployee
    
    OBJECTS d = CashDocument
    PROPERTIES(d) dateTime, seriesNumber, corrAccount
    PROPERTIES    sumIncome 'Приход' = sumCash[IncomeCashOrder](d),
                  sumOutcome 'Расход' = sumCash[IncomeCashOrder](d)
        
    ORDERS dateTime(d)
;
