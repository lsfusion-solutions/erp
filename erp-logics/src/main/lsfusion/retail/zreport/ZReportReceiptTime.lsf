MODULE ZReportReceiptTime;

REQUIRE ZReport;

NAMESPACE ZReport;

dateTimeOpen 'Время открытия' = DATA DATETIME (Receipt);
dateTimeClosed 'Время закрытия' = DATA DATETIME (Receipt);

EXTEND FORM zReports
    PROPERTIES (b) READONLY dateTimeOpen, dateTimeClosed
;

EXTEND FORM zReport
    PROPERTIES (b) READONLY dateTimeOpen, dateTimeClosed
;