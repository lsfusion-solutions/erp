MODULE RangeSimpleWarehouse;

REQUIRE RangeSimple, RangeWarehouse;

NAMESPACE Range;

in (Sku s, Range r, Warehouse w, DATE d) += dataIn(rangeRev(r, d), s) AND in(r, w);

inRangeSimpleWarehouse(Stock st) += st IS Warehouse;

EXTEND FORM warehouseRange
    FILTERS include(sr, ss, d)
    FILTERS include(rn, ds, sk, d)
;


