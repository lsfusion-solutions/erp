MODULE SerialPort;

REQUIRE SystemEvents;

NAMESPACE Utils;

initSerialPortJS INTERNAL CLIENT 'serialport.js';
initSerialPortCSS INTERNAL CLIENT 'serialport.css';

serialPortSend INTERNAL CLIENT 'serialPortSend(port,value)' (JSON, STRING);

onWebClientStarted() + {
    initSerialPortJS();
    initSerialPortCSS();
}
