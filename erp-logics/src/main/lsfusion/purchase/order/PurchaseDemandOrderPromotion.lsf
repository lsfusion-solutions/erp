MODULE PurchaseDemandOrderPromotion;

REQUIRE PurchaseDemandOrder, PriceListPromotion;

NAMESPACE Purchase;

EXTEND FORM demandOrder
    FILTERGROUP promotion
        FILTER 'В акции' filterPromotion(s, st)
;

backgroundDemand(Sku s, Stock st) += WHEN promotionPriceListDetailA(s, st) THEN colorPromotion(s, st);