FORM sendMessage FORMEXTID 'Message'
    PROPERTIES ATTR = 'http://www.1c.ru/SSL/Exchange/Message' EXTID 'xmlns:msg'
    PROPERTIES ATTR = 'http://www.w3.org/2001/XMLSchema' EXTID 'xmlns:xs'
    PROPERTIES ATTR = 'http://www.w3.org/2001/XMLSchema-instance' EXTID 'xmlns:xsi'

    OBJECTS configuration = Configuration
    PROPERTIES (configuration) IN header format EXTID 'msg:Format', currentDateTimeString EXTID 'msg:CreationDate'
    PROPERTIES (configuration) IN confirmation exchangePlan EXTID 'msg:ExchangePlan', toBase EXTID 'msg:To', fromBase EXTID 'msg:From', messageNo EXTID 'msg:MessageNo', receivedNo EXTID 'msg:ReceivedNo'
    
    OBJECTS availableVersion = AvailableVersion EXTID 'msg:AvailableVersion' IN header
    PROPERTIES (availableVersion) name EXTID 'value'

    OBJECTS objectExchange = ObjectExchange EXTID 'msg:ObjectType' IN availableObjectTypes
    PROPERTIES  name(objectExchange) EXTID 'msg:Name', inSendingTag(objectExchange, configuration) EXTID 'msg:Sending', inReceivingTag(objectExchange, configuration) EXTID 'msg:Receiving'
    FILTERS inSending(objectExchange, configuration) OR inReceiving(objectExchange, configuration) OR overIn(objectExchange, configuration)
    
    PROPERTIES (configuration) IN header prefix EXTID 'msg:Prefix'
    
    PROPERTIES emptyBody15(configuration) EXTID '=http://v8.1c.ru/edi/edi_stnd/EnterpriseData/1.5:Body'
    PROPERTIES emptyBody16(configuration) EXTID '=http://v8.1c.ru/edi/edi_stnd/EnterpriseData/1.6:Body'
;

idReceivedTicket = DATA LOCAL INTEGER ();
idReceivedTemp = DATA LOCAL INTEGER ();

FORM receiveMessage FORMEXTID 'Message'
    PROPERTIES () IN confirmation idReceivedTemp EXTID 'msg:MessageNo', idReceivedTicket EXTID 'msg:ReceivedNo'
;

overReceive ABSTRACT LIST (Configuration);
overReceive ABSTRACT LIST (Configuration, XMLFILE, INTEGER);
overImport ABSTRACT LIST (Configuration);

beforeImport ABSTRACT LIST (Configuration);
afterImport ABSTRACT LIST (Configuration);
errorImport ABSTRACT LIST (Configuration);

generateUid 'Генерировать UID' () {
    NEWSESSION {
        FOR EnterpriseData e IS EnterpriseData AND NOT uid(e) DO {
            generateUUID();
            uid(e) <- STRING[36](generatedUUID());
        }
        APPLY;
    }
} CONFIRM;

turnOutConstraints 'Отключить ограничения' = DATA BOOLEAN (Configuration);

CLASS Import1CLog 'Лог импорта 1С';
TABLE import1CLog (Import1CLog);

createImport1CLog 'Писать "Лог импорта 1С"' = DATA BOOLEAN (Configuration);
countDaysImport1CLog 'К-во дней для хранения  "Лог импорта 1С"' = DATA INTEGER (Configuration);
countDaysImport1CLogOver 'К-во дней для хранения  "Лог импорта 1С"' (Configuration c) = OVERRIDE countDaysImport1CLog(c), 180;
configuration = DATA Configuration(Import1CLog) INDEXED;
dateTime 'Дата/время изменения' = DATA DATETIME (Import1CLog);
date 'Дата изменения' (Import1CLog l) = DATE(dateTime(l)) MATERIALIZED INDEXED;
time 'Время изменения' (Import1CLog l) = TIME(dateTime(l));

user 'Пользователь' = DATA User (Import1CLog);
nameUser 'Пользователь' (Import1CLog l) = name(user(l));
idReceived 'Номер принятого сообщения' = DATA INTEGER (Import1CLog);
isReceived 'Сообщение принято' = DATA BOOLEAN (Import1CLog);
idTicket 'Получена квитанция' = DATA INTEGER (Import1CLog);
text 'Сообщение' = DATA TEXT (Import1CLog);

log1C = DATA LOCAL Import1CLog(Configuration);

deleteImport1CLog 'Очистить "Лог импорта 1С"' (Configuration c) {
    NEWSESSION {        
        DELETE Import1CLog log WHERE date(log)<=sum(currentDate(), -countDaysImport1CLogOver(c)) AND configuration(log)==c;
        APPLY;
    }
}
deleteImport1CLog 'Очистить "Лог импорта 1С"'(){
    FOR countDaysImport1CLogOver(Configuration c) DO NEWSESSION {
        DELETE Import1CLog log WHERE date(log)<=sum(currentDate(), -countDaysImport1CLogOver(c)) AND configuration(log)==c;
        APPLY;
    }    
}

receiveMessage 'Получить' (Configuration c) {
    NEWSESSION {
        LOCAL f = XMLFILE ();
        TRY {
            beforeImport(c);
            READ receivePath(c) + '.xml' TO f;
            IMPORT receiveMessage XML FROM f();
            overImport(c);
            log1C(c)<- NULL;            
            IF idReceivedTemp() AND NOT idReceivedTemp() <= idReceived(c) THEN {
                IF createImport1CLog(c) THEN NEWSESSION  NESTED LOCAL {
                    NEW l = Import1CLog {
                        configuration(l)<- c;
                        dateTime(l)<- currentDateTime();
                        user(l)<- currentUser();
                        idReceived(l) <- idReceivedTemp();
                        isReceived(l) <- TRUE;
                        idTicket(l)<- idReceivedTicket();
                    }
                    APPLY NESTED LOCAL;
                }
                idReceived(c) <- idReceivedTemp();
                isReceived(c) <- TRUE;
                overReceive(c);
                log1C(c)<- NULL;
                overReceive(c, f(), idReceivedTemp());
                in (c, EnterpriseData e) <- NULL WHERE packet(c, e) <= idReceivedTicket();
                idTicket(c) <- idReceivedTicket();
            } ELSE {
                isReceived(c) <- NULL;
                IF createImport1CLog(c) THEN NEWSESSION  NESTED LOCAL {
                    NEW l = Import1CLog {
                        configuration(l)<- c;
                        dateTime(l)<- currentDateTime();
                        user(l)<- currentUser();
                        idReceived(l) <- idReceivedTemp();
                        isReceived(l) <- NULL;
                        idTicket(l)<- idReceivedTicket();
                    }
                    APPLY NESTED LOCAL;
                }
            }
            IF turnOutConstraints(c) THEN setNoCancelInTransaction();
            afterImport(c);
            APPLY; 
        } CATCH {
            errorImport(c);
            MESSAGE System.messageCaughtException();
            NEWSESSION APPLY { isReceived(c) <- NULL; }
            IF createImport1CLog(c) THEN NEWSESSION  NESTED LOCAL {
                NEW l = Import1CLog {
                    configuration(l)<- c;
                    dateTime(l)<- currentDateTime();
                    user(l)<- currentUser();
                    idReceived(l) <- idReceivedTemp();
                    isReceived(l) <- NULL;
                    idTicket(l)<- idReceivedTicket();
                    text(l)<- CONCAT  '\n', text(l), System.messageCaughtException();
                }
                APPLY NESTED LOCAL;
            }
        }
    }
}

overSend ABSTRACT LIST (Configuration);
overSend ABSTRACT LIST (Configuration, XMLFILE);
beforeSend ABSTRACT LIST (Configuration);
beforeExport ABSTRACT LIST (Configuration);
afterExport ABSTRACT LIST (Configuration);
errorExport ABSTRACT LIST (Configuration);


scheme 'Схема' = DATA FILE (Configuration);
loadScheme 'Загрузить схему ' (Configuration c)  { INPUT =scheme(c) CHANGE; }
openScheme'Открыть схему' (Configuration c)  { open(scheme(c)); }
deleteImage 'Удалить схему'(Configuration c)  {
    scheme(c) <- NULL;
}
hasScheme 'Схема задана' (Configuration c) = TRUE IF scheme(c) MATERIALIZED;

sendMessage 'Передать' (Configuration c) {
    NEWSESSION {
        TRY {
            prevMessageNumber(c) <- messageNo(c);
            idSended(c) <- messageNo(c) (+) 1;
            LOCAL f = XMLFILE ();
            inLocalSending(c, EnterpriseData e) <- NULL WHERE e IS EnterpriseData;
            beforeSend(c);
            inLocalSending(c, EnterpriseData e) <- TRUE WHERE in(c, e);
            prevPacket(c, EnterpriseData e) <- packet(c, e) WHERE inLocalSending(c, e);
            packet(c, EnterpriseData e) <- messageNo(c) WHERE inLocalSending(c, e);
            //APPLY NESTED LOCAL;
            beforeExport(c);
            EXPORT sendMessage OBJECTS configuration = c XML TO f;
            IF hasScheme(c) THEN {
                TRY{
                    validateXML(scheme(c), FILE(f()));
                } CATCH {
                    MESSAGE (CONCAT '\n','Файл не соответствует схеме',(CONCAT '\n\n', validateError(), System.messageCaughtException()))  NOWAIT;    
                }
            } 
            WRITE f() TO sendPath(c);
            afterExport(c);
            isSended(c) <- TRUE;
//            packet(c, EnterpriseData e) <- messageNo(c) WHERE inLocalSending(c, e);
            overSend(c);
            overSend(c, f());
            APPLY NESTED LOCAL;
        } CATCH {
            MESSAGE System.messageCaughtException() NOWAIT;
            errorExport(c);
            packet(c, EnterpriseData e) <- prevPacket(c, e) WHERE inLocalSending(c, e);
            idSended(c) <- prevMessageNumber(c);
            
            APPLY;
            IF System.applyMessage() THEN {
                MESSAGE System.applyMessage() NOWAIT;
            }
            NEWSESSION APPLY { isSended(c) <- NULL; }
            CANCEL;
        }
    }
}

repeatIn (Configuration c, EnterpriseData e) {
    INPUT b = BOOLEAN DO {
        in(c, e) <- b;
        IF b THEN {
            packet(c,e) <- NULL;
        }
    } 
}

objectExchange = ABSTRACT VALUE ObjectExchange(EnterpriseData);

FORM objectExchange 'Настройки синхронизации'
    OBJECTS c = Configuration, o = ObjectExchange
    PROPERTIES name(o) READONLY, inSending(o, c), inReceiving(o, c)
    ORDERS name(o)
    FILTERGROUP inExchange
        FILTER 'Передается/принимается' inSending(o, c) OR inReceiving(o, c) DEFAULT
    
    OBJECTS e = EnterpriseData
    PROPERTIES desc(e, c) READONLY, packet(c,e) READONLY, in(c, e) ON CHANGE repeatIn(c, e)
    FILTERS inSending(c, e) OR in(c, e), NOT skip(c, e)
    FILTERGROUP inSending
        FILTER 'Вкл.' in(c, e)
    FILTERGROUP objectExchange
        FILTER 'По объекту синхронизации' objectExchange(e)=o DEFAULT
;

showOptions 'Настройки' (Configuration c) {
    SHOW objectExchange OBJECTS c = c NEWSESSION;
}

FORM configuration 'Конфигурация'
    OBJECTS c = Configuration PANEL
    PROPERTIES (c) name, dateStartExchange, toBase, path

    EDIT Configuration OBJECT c;
;

FORM configurations 'Конфигурации'
    OBJECTS c = Configuration
    PROPERTIES (c) name
    LIST Configuration OBJECT c
;

EXTEND FORM integrationData
    OBJECTS c = Configuration

    PROPERTIES () someConfigurations, fromBase, path

    PROPERTIES (c) PANEL
               panelIdSended = idSended BACKGROUND (IF isSended(c) THEN RGB(225,255,225) ELSE RGB(255, 225, 225)),
               panelIdReceived = idReceived BACKGROUND (IF isReceived(c) THEN RGB(225,255,225) ELSE RGB(255, 225, 225)),
               panelDataStartExchange = dateStartExchange, panelToBase = toBase,
               panelCreateImport1CLog = createImport1CLog, panelCountDaysImport1CLog= countDaysImport1CLogOver
    
    PROPERTIES (c) SHOWIF someConfigurations() 
               name,
               idTicket BACKGROUND (IF isSended(c) THEN RGB(225,255,225) ELSE RGB(255, 225, 225)),
               idSended BACKGROUND (IF isSended(c) THEN RGB(225,255,225) ELSE RGB(255, 225, 225)),
               idReceived BACKGROUND (IF isReceived(c) THEN RGB(225,255,225) ELSE RGB(255, 225, 225)),
               dateStartExchange, fromBase, toBase, path, nameVersion,
               createImport1CLog, countDaysImport1CLogOver
    PROPERTIES (c) SHOWIF someConfigurations() NEWSESSION NEW, EDIT, DELETE
    
    PROPERTIES sendMessage(c), receiveMessage(c), showOptions(c), generateUid() DRAW c    
    PROPERTIES (c) SHOWIF someConfigurations() hasScheme
    PROPERTIES (c) GRID loadScheme SHOWIF (someConfigurations() AND NOT hasScheme(c)), deleteImage SHOWIF (hasScheme(c) AND someConfigurations())
    OBJECTS  log = Import1CLog
    PROPERTIES (log) READONLY date, time, nameUser, idReceived, idTicket, text PANEL
    PROPERTIES  READONLY isError 'Ошибка' = TRUE IF text(log)
    PROPERTIES deleteImport1CLog(c) TOOLBAR DRAW log

    FILTERS configuration(log) == c
;

DESIGN integrationData {
    pane {
        NEW enterpriseData {
            caption = 'Обмен 1С';
            MOVE BOX (c);
            NEW status {
                horizontal = TRUE;
                MOVE PROPERTY (panelIdSended);
                MOVE PROPERTY (panelIdReceived);
            }
            NEW basic {
                horizontal = TRUE;      
                MOVE PROPERTY (panelDataStartExchange);
                MOVE PROPERTY (panelToBase);
                MOVE PROPERTY (fromBase());
            }
            NEW path {
                horizontal = TRUE;
                MOVE PROPERTY (path());
            }                
            NEW cGrid {
                fill = 1;
                MOVE BOX (c);
            }
            NEW buttons {
                horizontal = TRUE;
                MOVE PROPERTY (sendMessage(c));
                MOVE PROPERTY (receiveMessage(c));
                MOVE PROPERTY (generateUid());
                MOVE PROPERTY (showOptions(c));
                MOVE PROPERTY (someConfigurations());
            }
            NEW alignPane {
                tabbed = TRUE;
                fill = 1;
                NEW log{
                    fill = 1;
                    horizontal = TRUE;
                    caption = 'Лог';
                    showIf = createImport1CLog(c);
                    MOVE BOX(log){fill = 2;}
                    MOVE PROPERTY (text(log)){panelCaptionVertical = TRUE;alignment = STRETCH;}
                }
            }
        }
    }
}

