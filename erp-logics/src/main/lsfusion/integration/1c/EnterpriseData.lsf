META defStaticType (id, caption)
    CLASS ###id caption;
    TABLE ###id (###id);
    dataName '{master.data.name}' = DATA BPISTRING[100](###id);
    name '{master.data.name}' (###id o) = (OVERRIDE dataName(o), staticCaption(o) IF o IS ###id) CHARWIDTH 15;

    FORM dialog###id##s caption
        OBJECTS o = ###id
        PROPERTIES(o) READONLY name

        LIST ###id OBJECT o
    ;
    id (STRING s) = GROUP AGGR ###id id BY name(id);
END


META defStaticTypeValue (class, id, caption)
    EXTEND CLASS ###class { id caption };
    is###id###class (###class class) = class = ###class.id;
    is###id###class (STRING s) = is###id###class(class(s));
END

