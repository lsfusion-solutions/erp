MODULE CountryDefault;

REQUIRE Country, CurrencyDefault;

NAMESPACE Country;

// ------------------------------------------------ Стандартные значения --------------------------------------- //

loadDefaultCountry 'Добавить страны'(ISTRING[50] string, BPSTRING[3] number, ISTRING[50] stringA, BPSTRING[2] stringB, BPSTRING[3] stringC, BPSTRING[3] shortCurrency)  { //, value) {
    FOR number IS BPSTRING[3] AND NOT country(number) INLINE NEW c = Country DO {
       name(c) <- string;
       sid(c) <- number;
       nameOrigin(c) <- stringA;
       sidOrigin2(c) <- stringB;
       sidOrigin3(c) <- stringC;
       currency(c) <- currencyShortName(shortCurrency);
    }
}

