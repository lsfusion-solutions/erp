MODULE BinItem;

REQUIRE Bin, Item;

NAMESPACE Bin;

setBins (Item i) {
    SHOW changeBinsSku OBJECTS s=i;
}

EXTEND FORM item
    PROPERTIES bins(i) ON CHANGE setBins(i)
;

DESIGN item {
    itemPrimaryColumn2 {
        MOVE GROUP(itemWMS,i) {
            fill=1;
            lines = 2;
            PROPERTY(bins(i)){alignment = STRETCH;}
        }
    }
}
