    loadLookup('manufacturer_name', [ GROUP CONCAT TEXT('"' + TEXT(Manufacturer m) + '":"' + escapeJSONValue(name(m))  + '"'), ',' ORDER m]());
    loadLookup('country_name', [ GROUP CONCAT TEXT('"' + TEXT(Country c) + '":"' + escapeJSONValue(name(c))  + '"'), ',' ORDER c]());
