MODULE DisparityPharmacy;

REQUIRE Disparity, StockPharmacy;

NAMESPACE Disparity;

importCountryDispariry = DATA Country (DisparityDetail);

importCountryDispariry(DisparityDetail detail) <- importCountry(batch(detail)) WHEN CHANGED(batch(detail));
importCountry(InputDisparityDetail batch) += PREV(importCountryDispariry(disparityDetail(batch)));

overReplace(Country s, Country d) + { importCountryDispariry(DisparityDetail detail) <- s WHERE importCountryDispariry(detail) == d;}

seriesPharmacyDispariry = DATA STRING[20] (DisparityDetail);

seriesPharmacyDispariry(DisparityDetail detail) <- prevSeriesPharmacy(batch(detail)) WHEN CHANGED(batch(detail));
seriesPharmacy(InputDisparityDetail batch) += seriesPharmacyDispariry(disparityDetail(batch));

contractPriceDispariry = DATA STRING[20] (DisparityDetail);

contractPriceDispariry(DisparityDetail detail) <- prevContractPrice(batch(detail)) WHEN CHANGED(batch(detail));
contractPrice(InputDisparityDetail batch) += contractPriceDispariry(disparityDetail(batch));


    
    

    
