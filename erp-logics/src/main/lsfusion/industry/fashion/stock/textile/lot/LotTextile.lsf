MODULE LotTextile;

REQUIRE Lot;

NAMESPACE Lot;

eaeuLot 'ЕАЭС' = DATA BOOLEAN (Lot);

EXTEND FORM lot
    PROPERTIES(l) eaeuLot
;

DESIGN lot {
    ids {
        MOVE PROPERTY(eaeuLot(l));
    }
}

EXTEND FORM lots
    PROPERTIES(l) READONLY eaeuLot
;

DESIGN options {
    pane {
        NEW lotInit  {
            caption = 'Маркировка';
        }
    }
}
