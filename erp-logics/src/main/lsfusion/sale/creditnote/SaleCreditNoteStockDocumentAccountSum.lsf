MODULE SaleCreditNoteStockDocumentAccountSum;

REQUIRE SaleUserCreditNoteSaleLedger, SkuLedgerSum, SaleCreditNoteSkuLedger;

NAMESPACE Sale;

costSum[SaleLedger](UserCreditNoteDetail ledger) += WHEN ledger IS UserCreditNoteDetail THEN -NUMERIC[16,3](shipmentSum(ledger));
costSum[SkuLedger](UserCreditNoteDetail ledger) += -shipmentSum(ledger);

overSumItem(UserCreditNote ledger) += NUMERIC[18,4](shipmentSumUserCreditnoteDetail(ledger));
