MODULE SaleShipmentLotIn;

REQUIRE SaleShipmentLot;

NAMESPACE Sale;

beforeInputLotBarcode(UserInvoiceDetail d, STRING[200] s) + {
    IF NOT lot(barcodeToId(s)) THEN {
        NEW l = Lot {
            id(l) <- barcodeToId(s);
            tail(l) <- barcodeToTail(s);
            sku(l) <- sku(d);
        }
    }
}