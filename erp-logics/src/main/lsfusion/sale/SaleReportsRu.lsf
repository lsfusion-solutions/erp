MODULE SaleReportsRu;

REQUIRE SaleReports, SaleInvoice, SaleReturnInvoice, SaleOperation, PricingSum, SaleFullPrice, 
        SaleShipment, TaxItem, ZReport, Warehouse, PurchaseOrder, WriteOff, SaleLedgerWeek,
        Inventory, LegalEntityList, SkuList, StockList;

NAMESPACE SaleReports;

articleRu = ABSTRACT ISTRING[20](Sku);
priceSku 'Учетная цена' = ABSTRACT NUMERIC[17,5](Sku, Stock, DATE);
costPriceSku 'Себестоимость' = ABSTRACT NUMERIC[17,5](Sku, Stock, DATE);

supplierSku 'Поставщик товара' = ABSTRACT LegalEntity (Sku);
nameSupplierSku 'Наименование поставщика' (Sku s) = name(supplierSku(s));
idSupplierSku 'Код поставщика' (Sku s) = id(supplierSku(s));

inSessionSku 'Отм.' (Sku s) = OVERRIDE in(s), TRUE IF (GROUP SUM 1 IF inSession(group(GroupType t, s)));
printInSessionSku 'Товары' () =
    GROUP CONCAT name(Sku s) IF inSessionSku(s) ,', ' ORDER s CHARWIDTH 100;

inSaleReportLegalEntities 'Покупатели' () =
    GROUP CONCAT name(LegalEntity le) IF in(le) ,', ' ORDER le CHARWIDTH 30;

FORM dialogLegalEntitiesSaleReport 'Выбор покупателя'
    TREE legalEntityGroupTree lg = LegalEntityGroup PARENT parent(lg)
    PROPERTIES READONLY lgTreeName = name(lg)

    OBJECTS l = LegalEntity
    PROPERTIES(l) in
    PROPERTIES(l) READONLY name, id SHOWIF showIDs(), fullName, nameLegalEntityGroup
    ORDERS name(l)
    FILTERGROUP inactiveLegalEntity FILTER 'Активная' active(l) 'shift F10' DEFAULT
    FILTERGROUP include FILTER 'Отмеченные' in(l) 'F9'
    FILTERS isParent(legalEntityGroup(l), lg) OR
        l IS LegalEntity AND NOT lg IS LegalEntityGroup OR
        l IS LegalEntity AND lg IS LegalEntityGroup AND NOT legalEntityGroup(l)
;

DESIGN dialogLegalEntitiesSaleReport {
    BOX {
        NEW topContainer {
            horizontal = TRUE;
            fill = 1;

            MOVE BOX(TREE legalEntityGroupTree){
                caption = 'Группы организаций';
            }
            MOVE BOX(l) {
                fill = 3;
                GRID(l) {
                    defaultComponent = TRUE;
                }
            }
        }
        MOVE TOOLBARBOX;
    }
}

changeStocksSalesReport(GroupType gt, DATE df, DATE dt)  {
    DIALOG dialogStocksSaleReport DO {
        fillSaleReportFromTo(gt, df, dt);
    }
}


inSupplier 'Отм.' = DATA LOCAL BOOLEAN (LegalEntity);
inSaleReportSupplier 'Поставщики' () =
    GROUP CONCAT name(LegalEntity le) IF inSupplier(le) ,', ' ORDER le CHARWIDTH 30;

legalEntityList = DATA LOCAL LegalEntityList();
nameLegalEntityList 'Список контрагентов' () = name(legalEntityList());

changeLegalEntity 'Список контрагентов'() {
    DIALOG legalEntityLists OBJECTS o INPUT DO {
        legalEntityList() <- o;
        inSupplier(LegalEntity s) <- in(s, o);
    }
}

deleteIncludeLegalEntities 'Сбросить список'()  {
    legalEntityList() <- NULL;
    inSupplier(LegalEntity l) <- NULL;
} ASON CONTEXTMENU nameLegalEntityList;

FORM dialogLegalEntitiesSupplier 'Выбор поставщика'
    TREE legalEntityGroupTree lg = LegalEntityGroup PARENT parent(lg)
    PROPERTIES READONLY lgTreeName = name(lg)

    OBJECTS l = LegalEntity
    PROPERTIES(l) inSupplier READONLYIF legalEntityList()
    PROPERTIES(l) READONLY name, id SHOWIF showIDs(), fullName, nameLegalEntityGroup
    PROPERTIES changeLegalEntity() TOOLBAR
    ORDERS name(l)
    FILTERGROUP inactiveLegalEntity FILTER 'Активная' active(l) 'shift F10' DEFAULT
    FILTERGROUP inSupplier FILTER 'Отмеченные' in(l) 'F9'
    FILTERS isParent(legalEntityGroup(l), lg) OR
        l IS LegalEntity AND NOT lg IS LegalEntityGroup OR
        l IS LegalEntity AND lg IS LegalEntityGroup AND NOT legalEntityGroup(l)
;

DESIGN dialogLegalEntitiesSupplier {
    BOX {
        NEW topContainer {
            horizontal = TRUE;
            fill = 1;

            MOVE BOX(TREE legalEntityGroupTree){
                caption = 'Группы организаций';
            }
            MOVE BOX(l) {
                fill = 3;
                GRID(l) {
                    defaultComponent = TRUE;
                }
            }
        }
        MOVE TOOLBARBOX;
    }
}

inSaleOperation 'Отм.' = DATA LOCAL BOOLEAN (Sale.Operation);
nameInOperations 'Операции' = GROUP CONCAT name(Sale.Operation o) IF inSaleOperation(o), ', ' ORDER name(o), o;

FORM selectOperations 'Выберите операции'
    OBJECTS o = Sale.Operation
    PROPERTIES (o) inSaleOperation, name READONLY
    ORDERS name(o)
;

stockList = DATA LOCAL StockList();
nameStockList 'Список подразделений' () = name(stockList());

changeStockList 'Список подразделений'() {
    DIALOG stockLists OBJECTS o INPUT DO {
        stockList() <- o;
        inSaleReportDialog(Stock s) <- in(o, s);
    }
}

deleteIncludeStockLists 'Сбросить список'()  {
    stockList() <- NULL;
    inSaleReportDialog(Stock s) <- NULL;
} ASON CONTEXTMENU nameStockList;

FORM dialogStocksSaleReportRu 'Выбор складов'

    TREE stockTree a = BPSTRING[3], sg = StockGroup PARENT parent(sg)
    PROPERTIES READONLY VALUE(a), name(sg)
    ORDERS name(sg)
    FILTERS stringEqualsAll(a)

    OBJECTS s = Stock
    PROPERTIES(s) READONLY id, name, address, nameRegion
    PROPERTIES (s) inSaleReportDialog READONLYIF stockList()
    PROPERTIES changeStockList() TOOLBAR
    ORDERS name(s)
    FILTERS isParent(sg, s) OR (s IS Stock AND NOT sg),
        isCompany(s),
        countCompanyStock(sg)

    FILTERGROUP inactiveStock FILTER 'Активный' active(s) 'ctrl F10' DEFAULT
    FILTERGROUP select FILTER 'Отм.' inSaleReportDialog(s) 'F9'

    EVENTS
        ON INIT {
            inSaleReportDialog(Stock st) <- inSaleReport(st);
        },
        ON OK {
            inSaleReport(Stock st) <- inSaleReportDialog(st);
        }
;

DESIGN dialogStocksSaleReportRu {
    BOX {
        size = (1024, 768);

        NEW topContainer {
            horizontal = TRUE;
            fill = 1;

            MOVE BOX(TREE stockTree) {
                caption = 'Склады';
            }

            MOVE BOX(s) {
                fill = 3;
                GRID(s) {
                    defaultComponent = TRUE;
                }
                PROPERTY(name(s)){
                    charWidth = 35;
                }
            }
        }

        MOVE TOOLBARBOX;
    }
}

changeStocksSalesReportRu(GroupType gt, DATE df, DATE dt, DATE dfc, DATE dtc)  {
    DIALOG dialogStocksSaleReportRu DO {
        fillSaleReportFromTo(gt, df, dt);
        fillSaleReportFromToCompare(gt, dfc, dtc);
    }
}

skuList = DATA LOCAL List();
nameSkuList 'Список товаров' () = name(skuList());

changeSkuList 'Список товаров'() {
    DIALOG lists OBJECTS o INPUT DO {
        skuList() <- o;
        in(Sku s) <- NULL;
        in(SkuGroup sg) <- NULL;
        FOR list(ListDetail ld) = o DO
        in(Sku s) <- TRUE WHERE s = sku(ld);
    }
}

deleteIncludeSkuLists 'Сбросить список'()  {
    skuList() <- NULL;
} ASON CONTEXTMENU nameSkuList;


