        BY extractWeek(date(l));
sumSkuSold 'Продано за неделю (сумма)' (Sku s, INTEGER week, DATE dateFrom, DATE dateTo) = GROUP SUM
    sum(ReceiptDetail l) IF date(l) >= dateFrom AND date(l) <= dateTo AND sku(l) = s AND inSaleReportDialog(departmentStore(l)) AND inSessionSku(sku(l)) AND filterSaleLeger(l)
        BY extractWeek(date(l));
sumPrice 'Продано за неделю (сумма)' (Sku s, INTEGER week, DATE dateFrom, DATE dateTo) = GROUP SUM
    round2(quantity(ReceiptDetail l)*priceSku(sku(l),departmentStore(l), date(l))) IF date(l) >= dateFrom AND date(l) <= dateTo AND sku(l) = s AND inSaleReportDialog(departmentStore(l)) AND inSessionSku(sku(l))
        BY extractWeek(date(l));

quantitySold 'Продано за неделю по товару (кол-во)' (INTEGER week, DATE dateFrom, DATE dateTo) = GROUP SUM
    quantity(ReceiptDetail l) IF date(l) >= dateFrom AND date(l) <= dateTo AND inSessionSku(sku(l)) AND inSaleReportDialog(departmentStore(l))
        BY extractWeek(date(l));
sumSkuSold 'Продано за неделю (сумма)' (INTEGER week, DATE dateFrom, DATE dateTo) = GROUP SUM
    sum(ReceiptDetail l) IF date(l) >= dateFrom AND date(l) <= dateTo AND inSessionSku(sku(l)) AND inSaleReportDialog(departmentStore(l))
        BY extractWeek(date(l));
sumPrice 'Продано за неделю (сумма)' (INTEGER week, DATE dateFrom, DATE dateTo) = GROUP SUM
    round2(quantity(ReceiptDetail l)*priceSku(sku(l),departmentStore(l), date(l))) IF date(l) >= dateFrom AND date(l) <= dateTo AND inSessionSku(sku(l)) AND inSaleReportDialog(departmentStore(l))
        BY extractWeek(date(l));
sumBalance(Sku s, DATE dt) =
    GROUP SUM (balanceA(s,DepartmentStore st, dt)) IF inSessionSku(s) AND inSaleReportDialog(st);

FORM saleReportSkuReceiptWeek 'Статистика продаж'
    OBJECTS dates = (df = DATE, dt = DATE) PANEL
    PROPERTIES df 'Дата с' = VALUE(df), dt 'Дата по' = VALUE(dt), inSaleReportStocks(),
        namePrint(), year(), nameSkuList(), nameStockList(), nameLegalEntityList()

    PROPERTIES countWeek(df,dt)

    OBJECTS s = Sku
    PROPERTIES(s) id, name, nameUOM, nameSupplierLastOrderBatch, nameSupplierSku

    OBJECTS w = INTEGER GRID
    PROPERTIES toString4(w)
    PROPERTIES quantitySold(s,w,df,dt) COLUMNS 'w' (w) HEADER 'Кол-во единиц',
        sumSkuSold(s,w,df,dt) COLUMNS 'w' (w) HEADER 'Сумма в баз.валюте в ценах документа',
        sumSku = sumSold(s,w,df,dt) COLUMNS 'w' (w) HEADER 'Сумма в баз.валюте в ценах документа',
        sumPrice(s,w,df,dt) COLUMNS 'w' (w) HEADER 'Сумма по учетной цене',
        quantitySold(w,df,dt) COLUMNS 'w' (w),
        sumSkuSold(w,df,dt) COLUMNS 'w' (w),
        sum = sumSold(w,df,dt) COLUMNS 'w' (w),
        sumPrice(w,df,dt) COLUMNS 'w' (w)
    PROPERTIES sk = GROUP SUM 1 IF sumSkuSold(s,w,df,dt) COLUMNS 'w' (w) HEADER (dateFromYearWeekDay(year(), w, 1) + ' - ' + dateFromYearWeekDay(year(), w, 7))
    PROPERTIES(s,df) sumBalance

    FILTERS quantitySold(w,df,dt) AND quantitySold(s,w,df,dt)
;

printSaleReportReceiptAllWeek 'Статистика по неделям(чеки)' (DATE dateFrom, DATE dateTo) {
    year() <- extractYear(dateFrom);
    PRINT saleReportSkuReceiptWeek OBJECTS df = dateFrom, dt = dateTo XLSX;
}   IMAGE 'print.png' IN print;

EXTEND FORM salesReport
    PROPERTIES(df,dt) printSaleReportReceiptWeek, printSaleReportInvoiceWeek, printSaleReportWeek, printSaleReportAllWeek, 
        printSaleReportInvoiceAllWeek, printSaleReportReceiptAllWeek
//    PROPERTIES nameInExportOperations() ON CHANGE {DIALOG selectOperation;}
;

DESIGN salesReport {
    salesContainer {
        NEW weekSale {
            caption = 'Продажи по неделям';
            lines = 2;
            MOVE PROPERTY(printSaleReportReceiptWeek(df,dt));
            MOVE PROPERTY(printSaleReportInvoiceWeek(df,dt));
            MOVE PROPERTY(printSaleReportWeek(df,dt));
        }
        NEW weekAllSale {
            caption = 'Статистика продаж по неделям';
            lines = 2;
//            MOVE PROPERTY(nameInExportOperations());
            MOVE PROPERTY(printSaleReportAllWeek(df,dt));
            MOVE PROPERTY(printSaleReportInvoiceAllWeek(df,dt));
            MOVE PROPERTY(printSaleReportReceiptAllWeek(df,dt));
        }
    }
}