MODULE LotRu;

REQUIRE Lot, LotTypeRu;

NAMESPACE Stock;

EXTEND FORM lot
    PROPERTIES (l) manufactureDate, expiryDate
;

DESIGN lot {
    params {
        MOVE PROPERTY (manufactureDate(l));
        MOVE PROPERTY (expiryDate(l));
    }
}

EXTEND FORM lots
    PROPERTIES (l) READONLY manufactureDate, expiryDate
;

EXTEND FORM dialogInLots
    PROPERTIES (l) READONLY manufactureDate, expiryDate
;

EXTEND FORM dialogOutLots
    PROPERTIES (l) READONLY manufactureDate, expiryDate
;