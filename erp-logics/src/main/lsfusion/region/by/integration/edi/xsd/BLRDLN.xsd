<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2015 sp2 (http://www.altova.com) by Anand (Home) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="BLRDLN">
		<xs:annotation>
			<xs:documentation>Электронное сообщение Товарная Накладная в Беларуси</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MessageHeader">
					<xs:annotation>
						<xs:documentation>Заголовок сообщения</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="DeliveryNote">
					<xs:annotation>
						<xs:documentation>Данные накладной</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SpecialPart">
					<xs:annotation>
						<xs:documentation>Особенная часть электронного документа</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CanonizationAlghorithm" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Идентификатор алгоритма канонизации внесенного в документ текста</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="255"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Signature" maxOccurs="50">
								<xs:annotation>
									<xs:documentation>Подраздел «ЭЦП»</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="SecurityID">
											<xs:annotation>
												<xs:documentation>Уникальный номер ЭЦП в документе</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:integer">
													<xs:totalDigits value="2"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="CertificateID" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Серийный номер сертификата открытого ключа подписи</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="256"/>
													<xs:minLength value="0"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="CertificateSubject" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Ф.И.О. автора подписи</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="150"/>
													<xs:minLength value="0"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="SecurityPartyID" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Серийный номер сертификата открытого ключа удостоверяющего центра, издавшего сертификат открытого ключа подписанта</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="512"/>
													<xs:minLength value="0"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="SecurityPartyName" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Наименование удостоверяющего центра, издавшего сертификат открытого ключа подписанта</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="175"/>
													<xs:minLength value="0"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="SigningTime" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Дата-время выработки ЭЦП                                                              Формат: ГГГГММДДЧЧММСС</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:integer">
													<xs:totalDigits value="14"/>
													<xs:minExclusive value="10000000000000"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="SignatureValue">
											<xs:annotation>
												<xs:documentation>ЭЦП</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DeliveryNote">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="DeliveryNoteType">
					<xs:annotation>
						<xs:documentation>Вид сообщения</xs:documentation>
						<xs:appinfo>Вид накладной</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="3"/>
							<xs:enumeration value="270"/>
							<xs:enumeration value="701"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DocumentID">
					<xs:annotation>
						<xs:documentation>Уникальный номер электронного сообщения (иного документа)</xs:documentation>
						<xs:appinfo>Номер электронного документа</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="35"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CreationDateTime">
					<xs:annotation>
						<xs:documentation>Дата и время создания накладной (иного документа)
				Формат: ГГГГММДДЧЧММСС	</xs:documentation>
						<xs:appinfo>Дата создания электронного документа</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:integer">
							<xs:totalDigits value="14"/>
							<xs:minExclusive value="10000000000000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="FunctionCode">
					<xs:annotation>
						<xs:documentation>Функция электронной накладной:
			1 = Отмена
9 = Оригинал                  31 = Копия			</xs:documentation>
						<xs:appinfo>Функция электронной накладной</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="3"/>
							<xs:enumeration value="1"/>
							<xs:enumeration value="9"/>
							<xs:enumeration value="31"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DeliveryNoteID">
					<xs:annotation>
						<xs:documentation>Номер накладной (иного документа):
							ССС-GGGGGGGGGGGGG-NNNNNNNNNN
						</xs:documentation>
						<xs:appinfo>Серия и номер накладной</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="43"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DeliveryNoteDate">
					<xs:annotation>
						<xs:documentation>Дата накладной (иного документа)</xs:documentation>
						<xs:appinfo>Дата накладной</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:integer">
							<xs:totalDigits value="8"/>
							<xs:minExclusive value="10000000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ContractName">
					<xs:annotation>
						<xs:documentation>Основание отпуска
				Наименование документа</xs:documentation>
						<xs:appinfo>Наименование основания отпуска</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="70"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ContractID">
					<xs:annotation>
						<xs:documentation>Основание отпуска
			Номер договора или другого документа		</xs:documentation>
						<xs:appinfo>Номер основания отпуска</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="70"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ContractDate" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Основание отпуска.
							Дата договора или другого документа
						</xs:documentation>
						<xs:appinfo>Дата основания отпуска</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:integer">
							<xs:totalDigits value="8"/>
							<xs:minExclusive value="10000000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Document" minOccurs="0" maxOccurs="100">
					<xs:annotation>
						<xs:documentation>Документы, переданные с товаром</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="DocumentID" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Номер документа</xs:documentation>
									<xs:appinfo>Номер документа, переданного с товаром</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0"/>
										<xs:maxLength value="70"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="DocumentDate" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Дата документа                                             Формат: ГГГГММДД</xs:documentation>
									<xs:appinfo>Дата документа, переданного с товаром</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:integer">
										<xs:totalDigits value="8"/>
										<xs:minExclusive value="10000000"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="DocumentName">
								<xs:annotation>
									<xs:documentation>Наименование документа, и если нужно, его описание</xs:documentation>
									<xs:appinfo>Название документа, переданного с товаром</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
										<xs:maxLength value="512"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="Shipper">
					<xs:annotation>
						<xs:documentation>Грузоотправитель</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Receiver">
					<xs:annotation>
						<xs:documentation>Грузополучатель</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="ShipFrom"/>
				<xs:element name="ShipTo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Пункт разгрузки</xs:documentation>
						<xs:appinfo>Пункт разгрузки</xs:appinfo>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="GLN" minOccurs="0">
								<xs:annotation>
									<xs:documentation>GLN пункта разгрузки</xs:documentation>
									<xs:appinfo>GLN пункта разгрузки</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0"/>
										<xs:maxLength value="13"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Address" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Адрес пункта разгрузки</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0"/>
										<xs:maxLength value="140"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="Carrier"/>
				<xs:element name="OrderID" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Номер заказа, к которому относится электронная накладная</xs:documentation>
						<xs:appinfo>Номер заказа, к которому относится электронная накладная</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="0"/>
							<xs:maxLength value="35"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CancelDeliveryNoteID" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Номер отмененной накладной (иного документа)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="0"/>
							<xs:maxLength value="70"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Currency">
					<xs:annotation>
						<xs:documentation>3-х буквенный код валюты: Приложение 23 к Решению Комиссии Таможенного союза от 20 сентября 2010 г. № 378</xs:documentation>
						<xs:appinfo>Код валюты</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="3"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ExtraField" minOccurs="0" maxOccurs="500">
					<xs:annotation>
						<xs:documentation>Дополнительные поля в накладной</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="FieldName">
								<xs:annotation>
									<xs:documentation>Название поля текстом</xs:documentation>
									<xs:appinfo>Название дополнительного поля в накладной</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
										<xs:maxLength value="255"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="FieldCode" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Код поля.</xs:documentation>
									<xs:appinfo>Код дополнительного поля в накладной</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0"/>
										<xs:maxLength value="6"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="FieldValue">
								<xs:annotation>
									<xs:documentation>Значение поля</xs:documentation>
									<xs:appinfo>Значение дополнительного поля в накладной</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
										<xs:maxLength value="2560"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="DespatchAdviceLogisticUnitLineItem"/>
				<xs:element name="Total">
					<xs:annotation>
						<xs:documentation>Итоговая информация</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="TotalAmountWithoutCharges">
								<xs:annotation>
									<xs:documentation>Общая сумма без НДС</xs:documentation>
									<xs:appinfo>Общая стоимость без НДС</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:totalDigits value="35"/>
										<xs:fractionDigits value="2"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="TotalAmountCharges" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Общая сумма НДС</xs:documentation>
									<xs:appinfo>Общая сумма НДС</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:totalDigits value="35"/>
										<xs:fractionDigits value="2"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="TotalAmount">
								<xs:annotation>
									<xs:documentation>Общая сумма с НДС</xs:documentation>
									<xs:appinfo>Общая стоимость с НДС</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:totalDigits value="35"/>
										<xs:fractionDigits value="2"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="TotalLineItem">
								<xs:annotation>
									<xs:documentation>Всего количество товарных позиций (количество Line)</xs:documentation>
									<xs:appinfo>Всего количество товарных позиций</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:integer">
										<xs:totalDigits value="18"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="TotalLineItemQuantity" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Всего количество единиц</xs:documentation>
									<xs:appinfo>Всего количество единиц</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:totalDigits value="18"/>
										<xs:fractionDigits value="5"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="TotalAmountExcise" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Всего сумма акциза</xs:documentation>
									<xs:appinfo>Всего сумма акциза</xs:appinfo>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:totalDigits value="35"/>
										<xs:fractionDigits value="2"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MessageHeader">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MessageID">
					<xs:annotation>
						<xs:documentation>Уникальный номер сообщения, который присваивается отправителем</xs:documentation>
						<xs:appinfo>Уникальный номер сообщения</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="14"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="MsgDateTime">
					<xs:annotation>
						<xs:documentation>Дата и время создания сообщения</xs:documentation>
						<xs:appinfo>Дата и время создания сообщения</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:integer">
							<xs:totalDigits value="14"/>
							<xs:minExclusive value="10000000000000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="MessageType">
					<xs:annotation>
						<xs:documentation>Тип сообщения</xs:documentation>
						<xs:appinfo>Тип сообщения</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="6"/>
							<xs:enumeration value="BLRDLN"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="MsgSenderID">
					<xs:annotation>
						<xs:documentation>GLN номер отправителя сообщения</xs:documentation>
						<xs:appinfo>GLN номер отправителя</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="13"/>
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="MsgReceiverID">
					<xs:annotation>
						<xs:documentation>GLN номер получателя сообщения</xs:documentation>
						<xs:appinfo>GLN номер получателя</xs:appinfo>
					</xs:annotation>
