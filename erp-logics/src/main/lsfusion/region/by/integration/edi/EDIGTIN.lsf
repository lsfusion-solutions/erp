MODULE EDIGTIN;

REQUIRE Item;

NAMESPACE EDI;

@defineDoubleItemAttribute(GTIN, STRING[14], 'GTIN', itemBase);

INDEX GTIN(Item i);

GTINBarcode 'GTIN' (Sku s) = OVERRIDE GTIN(s), idBarcode(s);
skuGTINa = GROUP MAX Sku s BY GTINBarcode(s);
skuGTIN(STRING[15] s) = OVERRIDE skuGTINa(s), skuBarcode(s);
