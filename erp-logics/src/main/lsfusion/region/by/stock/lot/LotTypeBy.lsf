MODULE LotTypeBy;

REQUIRE LotType;

NAMESPACE Stock;

EXTEND CLASS LotType  {
    shoes 'Код РБ идентификации товара (обувь, шины)'
}

filterEZLotType (LotType t) = t == LotType.shoes;
idExt (LotType t) = IF t == LotType.shoes THEN '20';

EXTEND FORM dialogLotTypes
    PROPERTIES(t) READONLY idExt BEFORE name(t)
    FILTERS filterEZLotType(t)
;

