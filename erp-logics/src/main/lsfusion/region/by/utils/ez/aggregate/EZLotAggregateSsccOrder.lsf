MODULE EZLotAggregateSsccOrder;

REQUIRE EZLotAggregateOrder;

NAMESPACE EZ;

EXTEND CLASS LotOrderKind {
    sscc 'SSCC'
};

aggregate (LotOrder o) += kind(o) == LotOrderKind.sscc;
overIdAggregateLot(LotOrderDetail d) += 'Не определен' IF kind(lotOrder(d)) == LotOrderKind.sscc;

lotTypeCheckSkip(LotOrder o) += kind(o) == LotOrderKind.sscc;
skipCheckDocumentLot(LotOrderDetail d) += kind(lotOrder(d)) == LotOrderKind.sscc;
skipChangedCheckLotOrderDetail(LotOrderDetail d)  += kind(lotOrder(d)) == LotOrderKind.sscc;