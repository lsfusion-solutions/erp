MODULE EZAggregationLotGenerator;

REQUIRE EZLotAggregateOrder;

NAMESPACE EZ;


FORM aggregationLotGenerator 'Генератор КА (создание)'

;

DESIGN aggregationLotGenerator {
    BOX {
        size = (1024, 768);

        NEW tabbedPane {
            tabbed = TRUE;
            fill = 1;
        }
    }
    REMOVE PROPERTY(formOk());
    MOVE TOOLBARBOX;
}

createAggregationLotOrder () + {
    SHOW aggregationLotGenerator;
}
