MODULE PricingPurchasePackagingCostManufacturingPrice;

REQUIRE PricingPurchasePackagingCost, PurchaseManufacturingPrice;

NAMESPACE Purchase;

extaraIncomeManufacturingPrice(InvoiceDetail d) += - packagingPrice(d);

overCopy(EInvoiceDetail ei, UserInvoiceDetail i) + {
    manufacturingMarkup(i) <- NUMERIC[8,4](extraFieldValueByCode(ei, 'PT0028'));
}
